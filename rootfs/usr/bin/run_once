#!/usr/bin/with-contenv bash

echo "**** running filebot one time ****"
echo "**** watch dir:      ${WATCH_DIR}"
echo "**** local dir:      ${LOCAL_DATA}"
echo "**** filebot mode:   ${FILEBOT_MODE}"
echo "**** plex token:     ${PLEX_TOKEN}"

exec s6-setuidgid abc /bin/bash \
  /app/filebot-add-to-plex.sh ${WATCH_DIR} ${LOCAL_DATA} ${FILEBOT_MODE} ${PLEX_TOKEN}
